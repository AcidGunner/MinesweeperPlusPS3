    <event eventtype="3" enumb="0">
      <action>
        <libid>1</libid>
        <id>603</id>
        <kind>7</kind>
        <userelative>0</userelative>
        <isquestion>0</isquestion>
        <useapplyto>-1</useapplyto>
        <exetype>2</exetype>
        <functionname></functionname>
        <codestring></codestring>
        <whoName>self</whoName>
        <relative>0</relative>
        <isnot>0</isnot>
        <arguments>
          <argument>
            <kind>1</kind>
            <string>if didwestart=1
    {
    timer += 1;
    
    // Fade logic
    if (fade_in) {
        alpha_val += fade_speed;
        if alpha_val&gt;0.3 &amp;&amp; phase==1 &amp;&amp; !show_part4 alarm[4]=1;
        
        if (alpha_val &gt;= 1) {
            alpha_val = 1;
            if phase==0 &amp;&amp; !show_part1 alarm[2]=1;
            if (timer &gt; show_time) {
                fade_in = false;
            }
        }
    } else {
        if phase==0
        {
            show_part1=false;
            show_part2=false;
            show_part3=false;
        }
        alpha_val -= fade_speed*4;
        alpha_val2 = alpha_val;
        if (alpha_val &lt;= 0) {
            alpha_val = 0;
            alpha_val2 = 0;
            phase += 1;
            if phase=1
            {
                audio_play_sound(boot2,1,false)
                show_time = global.FSP*4;
            }
            timer = 0;
            fade_in = true;
        }
    }
    
    test_count++;
    time=test_count/global.FSP
    
    alpha_test = alpha_val*1.3
    if alpha_test&gt;1 alpha_test=1;
    
    if show_part2=true
    {
        tsize=lerp(tsize,size-40,0.2);
        ty1=lerp(ty1,cy-20,0.2);
        ty2=lerp(ty2,cy+20,0.2);
        test_s=lerp(test_s,1,0.2);
        if test_s&gt;0.99 &amp;&amp; !show_part3 show_part3=true;
    }
    
    if show_part5==true
    {
        alpha_val2 += fade_speed;
        if (alpha_val2 &gt;= 1) {
            alpha_val2 = 1;
        }
    }
}
</string>
          </argument>
        </arguments>
      </action>
    </event>
    <event eventtype="8" enumb="0">
      <action>
        <libid>1</libid>
        <id>603</id>
        <kind>7</kind>
