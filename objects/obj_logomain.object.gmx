        <arguments>
          <argument>
            <kind>1</kind>
            <string>draw_set_alpha(alpha_val);
draw_set_color(c_white);
draw_set_halign(fa_center);
draw_set_valign(fa_middle);

if (phase == 0) {
    //background
    draw_clear(c_black);
    
    draw_set_font(fnt_smol);
    draw_set_alpha(1);
    if didwestart = 0
    {
        draw_text(cx, cy, "WARNING!

THIS PROJECT IS NOT AFFILIATED WITH
SONY COMPUTER ENTERTAINMENT!!

THANKS FOR UNDERSTANDING!!!");
    }
    
    draw_set_alpha(alpha_val);
    // Overlay fading background
    var col_bg_light = make_color_rgb(180,180,180); // light gray
    var col_bg_dark  = make_color_rgb(0,0,0);       // black
    
    // Interpolate between dark and light
    var final_col = merge_color(col_bg_dark, col_bg_light, alpha_test);
    
    // Fullscreen rectangle with that color
    draw_set_color(final_col);
    draw_rectangle(0, 0, room_width, room_height, false);
    draw_set_color(c_white);
    
    // Colors
    var col_top    = make_color_rgb(120, 120, 255);
    var col_side   = make_color_rgb(0, 0, 255);
    var col_bottom = make_color_rgb(0, 0, 120);
    var col_center = make_color_rgb(50, 50, 255);
    
    if show_part1=true
    {
        // --- Gradient Diamond with alpha ---
        draw_primitive_begin(pr_trianglelist);
        draw_vertex_color(cx, cy - size, col_top, alpha_val);
        draw_vertex_color(cx - size, cy, col_side, alpha_val);
        draw_vertex_color(cx, cy, col_center, alpha_val);
        draw_vertex_color(cx, cy - size, col_top, alpha_val);
        draw_vertex_color(cx, cy, col_center, alpha_val);
        draw_vertex_color(cx + size, cy, col_side, alpha_val);
        
        draw_vertex_color(cx, cy + size, col_bottom, alpha_val);
        draw_vertex_color(cx - size, cy, col_side, alpha_val);
        draw_vertex_color(cx, cy, col_center, alpha_val);
        draw_vertex_color(cx, cy + size, col_bottom, alpha_val);
        draw_vertex_color(cx, cy, col_center, alpha_val);
        draw_vertex_color(cx + size, cy, col_side, alpha_val);
        draw_primitive_end();
    }
    
    if show_part2=true
    {
        // --- Inner “S” cuts ---
        // Top cut (dark orange → lighter toward center)
        draw_primitive_begin(pr_trianglelist);
        draw_vertex_color(cx, ty1 - tsize, col_top, alpha_val);
        draw_vertex_color(cx - tsize, ty1, col_side, alpha_val);
        draw_vertex_color(cx, ty1, col_center, alpha_val);
        draw_vertex_color(cx, ty1 + tsize, col_bottom, alpha_val);
        draw_vertex_color(cx - tsize, ty1, col_side, alpha_val);
        draw_vertex_color(cx, ty1, col_center, alpha_val);
        draw_primitive_end();
        
        draw_primitive_begin(pr_trianglelist);
        draw_vertex_color(cx, ty2 - tsize, col_top, alpha_val);
        draw_vertex_color(cx, ty2, col_center, alpha_val);
        draw_vertex_color(cx + tsize, ty2, col_side, alpha_val);
        draw_vertex_color(cx, ty2 + tsize, col_bottom, alpha_val);
        draw_vertex_color(cx, ty2, col_center, alpha_val);
        draw_vertex_color(cx + tsize, ty2, col_side, alpha_val);
        draw_primitive_end();
    }
    
    if show_part3=true
    {
        // --- Text with alpha ---
        draw_set_font(fnt_main);
        draw_set_color(c_black);
        draw_set_alpha(alpha_val);
        draw_text(cx, cy, "AcidNT3.1# # # # #Productions");
    }
}
else if (phase == 1) {
    // PlayStation logo
    draw_set_font(fnt_smol);
    draw_set_color(c_white);
    draw_set_alpha(alpha_val);
    draw_sprite_ext(spr_logo,0,cx,cy,0.5,0.5,0,c_white,alpha_val);
    if show_part5==true
    {
        draw_set_alpha(alpha_val);
        draw_text(cx, cy - size + 10, "Made by");
        draw_text(cx, cy + size - 10, "AcidPS3 Project");
    }
    if show_part4==true
    {
        draw_set_font(fnt_smoler);
        draw_set_alpha(alpha_val);
        draw_text(cx, cy + size + 15, "The PS3 GameDEV revival project.#(Since 2025)");
    }
}

//reset
draw_set_halign(fa_left);
draw_set_valign(fa_top);
draw_set_alpha(1);
</string>
          </argument>
        </arguments>
      </action>
